pipeline 
{
agent any
tools
{
nodejs 'node 8.9.0'
}
stages
{
stage("build")
{
steps
{
echo 'Build result app'
dir('result')
{
sh 'npm install'
}
}
}
stage("test")
{
when
{
branch 'master'
changeset "**/result/**"
}
steps
{
echo 'Running Tests on result app'
dir('result')
{
sh 'npm install && npm test'
}
}
}
}
post
{
always
{
echo 'Building multibranch pipeline for result iscompleted..'
}
}
}
